<!DOCTYPE html>
<html>
  <head>
    <title>Whatshouldweeat</title>
    <%= csrf_meta_tags %>
    <script>
var map;
function initMap() {
  map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: 32.7156, lng: -117.1581},
    zoom: 8
  });
}
</script>
    <script src="/assets/jquery.js" type="text/javascript"></script>
    <script src="/assets/jquery_ujs.js" type="text/javascript"></script>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCkb6qe0T6dFkQqUtOg7_Wgkv0e7HwSFA4&callback=initMap"
    async defer></script>
	  <link href="https://fonts.googleapis.com/css?family=Press+Start+2P" rel="stylesheet">
    <link href="/assets/hover.css" rel="stylesheet" media="all">
    <link href='https://fonts.googleapis.com/css?family=Lobster+Two' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Bungee Shade' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Cuprum' rel='stylesheet'>
  </head>

  <body>
    <header>
      <h1 class="center header"><span class="main-title"> Kafood</span> </h1>
    </header>
    <h1>
      <% flash.each do |k, v| %>
        <div id="<%= k %>"><%= v %></div>
      <% end %></h1>
    <%= yield %>
  </body>
</html>
